<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DynamicBackgroundButton">
        <attr name="radius" format="dimension" />
        <attr name="colorActive" format="color" />
        <attr name="colorPressed" format="color" />
        <attr name="colorDisable" format="color" />
        <attr name="stroked" format="boolean" />
        <attr name="stroke" format="dimension" />
        <attr name="type" format="enum">
            <enum name="primary" value="1" />
            <enum name="secondary" value="2" />
            <enum name="tertiary" value="3" />
            <enum name="outlined" value="4" />
            <enum name="accent" value="6" />
        </attr>
    </declare-styleable>

</resources>